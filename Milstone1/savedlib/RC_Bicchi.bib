   editor = {Dario, Paolo and Sandini, Giulio and Aebischer, Patrick},
   publisher = {Springer Berlin Heidelberg},
   pages = {165-177},
   abstract = {It is a common observation that the human hand performs various manipulation tasks using not only its fingertips, but all the surfaces available for contact, i.e. intermediate phalanges and the palm. In the first part of this paper some such whole-hand operations are discussed, relating to different domains of fine manipulation such as grasping, exploration and micromotion of objects.},
   ISBN = {978-3-642-58069-7},
   type = {Conference Proceedings}
}

